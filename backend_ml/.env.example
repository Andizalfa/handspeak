# ================================
# BACKEND ML ENVIRONMENT VARIABLES
# Copy file ini ke .env dan isi dengan nilai yang sesuai
# ================================

# Hugging Face Configuration
# Format: username/repo-name
# Contoh: HF_REPO_ID=johndoe/bisindo-model
HF_REPO_ID=your_username/your_repo_name

# Force download from HuggingFace (true/false)
# Set true untuk selalu download model terbaru
FORCE_DOWNLOAD_FROM_HF=false

# Cache directory
HF_CACHE_DIR=./test_cache
